<section class="outprofile">
  <div class="outprofile-left">
    <img src="{{seller.image}}" alt="user">
  </div>
  <div class="outprofile-right">
    <h2>{{seller.username}}</h2>
    <p>{{seller.timeTable}}</p>
  </div>
</section>
<section id=list>
  <ul class="indexlist">
  {{#each products}}
    <li> 
      <div class="imagelist">
       
        <img src="{{this.image}}" alt="image">
    
       
      </div>
      <a href="/product/{{this._id}}">{{this.name}}</a>
      <p>{{this.price}}â‚¬</p>
    </li>
  {{/each}}
  </ul>
</section>

